{
  "expo": {
    "name": "Aslahah",
    "slug": "Aslahah",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/static/logo1.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/static/logo.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.furkan.Aslahah"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/static/logo_blue.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.furkan.Aslahah"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "e2a769f0-57ce-40a5-8f54-0b7fd6db86ec"
      }
    }
  }
}
